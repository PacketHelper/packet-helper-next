{
  "name": "packet-helper-next",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "start:debug": "node --inspect-brk=5858 ./src/main.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "gsap": "^3.7.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.21",
    "prettier": "^2.3.2",
    "vue": "^2.6.12",
    "vue-router": "^3.0.1",
    "vuetify": "^2.4.11",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.0-rc.12",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.12",
    "@vue/cli-service": "^3.0.0-rc.12",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "deepmerge": "^4.2.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "sass": "^1.32.12",
    "sass-loader": "^11.0.1",
    "semantic-release": "^17.4.4",
    "vue-cli-plugin-vuetify": "~2.4.0",
    "vue-template-compiler": "^2.6.12"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "plugin:prettier/recommended",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "release": {
    "branches": [
      "versioning"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
